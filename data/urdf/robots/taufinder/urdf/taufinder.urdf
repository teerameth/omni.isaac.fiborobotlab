<?xml version="1.0"?>
<robot name="taufinder">

<link name="base_link">
	<pose> 0 0 0 0 0 0</pose>
        <visual>
          <origin xyz="0 0 0.2" rpy="0 0 0" />
          <geometry>
            	<box size="0.5 0.5 0.4"/>
          </geometry>
        </visual>
        <inertial>
           	<mass value="3" />
            	<origin xyz="0 0 0.5" rpy="0 0 0" />
            	<inertia ixx="0.1025" iyy="0.125" izz="0.1025" ixy="0" ixz="0" iyz="0"/>
       </inertial>
      </link>

<link name="motor">
	<pose> 0 0 0 0 0 0</pose>
        <visual>
          <origin xyz="0 0 0" rpy="0 0 0" />
          <geometry>
            	<cylinder length="0.5" radius="0.25"/>
          </geometry>
        </visual>
        <inertial>
           	<mass value="1" />
            	<origin xyz="0 0 0.5" rpy="0 0 0" />
            	<inertia ixx="0.0364583" iyy="0.03645833" izz="0.03125" ixy="0" ixz="0" iyz="0"/>
       </inertial>
      </link>

<link name="motor_axis">
	<pose> 0 0 0 0 0 0</pose>
        <visual>
          <origin xyz="0 0 0" rpy="0 0 0" />
          <geometry>
            	<cylinder length="0.5" radius="0.1"/>
          </geometry>
        </visual>
        <inertial>
           	<mass value="0.2" />
            	<origin xyz="0 0 0.5" rpy="0 0 0" />
            	<inertia ixx="0.004666666" iyy="0.0046666" izz="0.001" ixy="0" ixz="0" iyz="0"/>
       </inertial>
      </link>

<link name="motor_rod">
	<pose> 0 0 0 0 0 0</pose>
        <visual>
          <origin xyz="0 0 0" rpy="0 0 0" />
          <geometry>
            	<box size="0.1 0.5 0.1"/>
          </geometry>
        </visual>
        <inertial>
           	<mass value="0.1" />
            	<origin xyz="0 0 0.5" rpy="0 0 0" />
            	<inertia ixx="0.0021666" iyy="0.0021666" izz="0.0001666" ixy="0" ixz="0" iyz="0"/>
       </inertial>
      </link>

<link name="motor_load">
	<pose> 0 0 0 0 0 0</pose>
        <visual>
          <origin xyz="0 0 0" rpy="0 0 0" />
          <geometry>
            	<box size="0.3 0.3 0.3"/>
          </geometry>
        </visual>
        <inertial>
           	<mass value="2" />
            	<origin xyz="0 0 0.5" rpy="0 0 0" />
            	<inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0" ixz="0" iyz="0"/>
       </inertial>
      </link>

<joint name ="motor" type="fixed">
        <origin xyz="0 0 0.650" rpy="0 1.57 0"/>
        <parent link="base_link"/>
        <child link="motor"/>
</joint>

<joint name ="motor_axis_joint" type="continuous">
        <origin xyz="0 0 0.5" rpy="0 0 0"/>
        <parent link="motor"/>
        <child link="motor_axis"/>
</joint>

<joint name ="motor_rod" type="fixed">
        <origin xyz="0 0.25 0" rpy="0 0 0"/>
        <parent link="motor_axis"/>
        <child link="motor_rod"/>
</joint>

<joint name ="motor_load" type="fixed">
        <origin xyz="0 0.4 0" rpy="0 0 0"/>
        <parent link="motor_rod"/>
        <child link="motor_load"/>
</joint>


</robot>
